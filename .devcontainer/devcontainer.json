{
	"name": "ClusterFuzz",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"features": {
		"ghcr.io/devcontainers/features/python:1": {
			"version": "3.11",
			"installPipenv": true
		},
		"ghcr.io/devcontainers/features/homebrew:1": {}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-python.vscode-pylance"
			]
		}
	},
	"remoteUser": "vscode"
}
